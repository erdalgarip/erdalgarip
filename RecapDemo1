public class Main {

	public static void main(String[] args) {
		// Recap Demo
		int sayi1 = 12;
		int sayi2 = 38;
		int sayi3 = 6;
		int enBuyuk = sayi1;
		
		
		if(enBuyuk<sayi2) {
			
		 enBuyuk = sayi2;	
		
 		}
		if(enBuyuk<sayi3) {
			 enBuyuk = sayi3;	
			
 		}
				
		System.out.println("En Büyük Sayı : "+enBuyuk);
		
		 
		

	}

}
