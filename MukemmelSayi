
public class Main {

	public static void main(String[] args) {
		// Mükemmel Sayiyi Bulma 
	int sayiSec = 6;
	int toplam = 0;
	for (int i = 1; i < sayiSec; i++) {
		
		if(sayiSec % i ==0) { 	
			toplam=toplam+i;	
			
	}
}

	if(sayiSec==toplam) {
		
		System.out.println(sayiSec+" bir mükemmel sayıdır.");	
	} else {
		
		System.out.println(sayiSec+" bir mükemmel sayı değildir.");	
	}
	
	

		
		
		
	}

}
